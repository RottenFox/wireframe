<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.StackPane?>

<TabPane fx:id="showpurchase" style="-fx-background-color: White;" styleClass="tab-pane2" stylesheets="@../looks.css" tabClosingPolicy="ALL_TABS" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.controller.AddpurchasetabController">
  <tabs>
    <Tab fx:id="tab" closable="false" text="Add">
         <graphic>
            <Button mnemonicParsing="false" onAction="#addtab" text="ADD NEW TAB" />
         </graphic>
    </Tab>
      <Tab text="Untitled Tab">
         <content>
            <StackPane fx:id="stackerPane" prefHeight="150.0" prefWidth="200.0" />
         </content>
      </Tab>
  </tabs>
</TabPane>
